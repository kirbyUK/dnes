{
    "name": "dnes",
    "description": "A NES emulator written in D",
    "homepage": "https://github.com/kirbyUK/dnes",
    "authors": [
        "Alex Kerr"
    ],
    "copyright": "Copyright Â© 2020, Alex Kerr",
    "license": "ISC",
    "dependencies": {
        "bindbc-sdl": "~>0.1.0"
    },
    "versions": ["SDL_2010"],
    "configurations": [
        {
            "name": "application",
            "targetType": "executable",
            "mainSourceFile": "source/app.d",
            "excludedSourceFiles": ["source/dnes/test/*"]
        },
        {
            "name": "unittest",
            "targetType": "executable",
            "targetName": "dnes-test",
            "mainSourceFile": "source/dnes/test/tests.d",
            "excludedSourceFiles": ["source/app.d"]
        }
    ]
}
